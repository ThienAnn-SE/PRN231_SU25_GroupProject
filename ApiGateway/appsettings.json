{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Fingerprint": {
    "RateLimitThreshold": 100,
    "RateLimitWindow": "00:15:00",
    "WhitelistedIps": [ "127.0.0.1", "::1" ],
    "KnownMaliciousIps": [],
    "EnablePatternDetection": true,
    "EnableThreatIntelligence": true,
    "EnableBotDetection": true
  }
}
